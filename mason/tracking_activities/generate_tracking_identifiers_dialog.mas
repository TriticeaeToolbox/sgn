<%args>

</%args>


<div class="modal fade" id="generate_tracking_identifiers_dialog" name="generate_tracking_identifiers_dialog" tabindex="-1" role="dialog" aria-labelledby="generateTrackingIdentifiersDialog">
    <div class="modal-dialog modal-xl" role="document">
        <div class="modal-content ui-front">
            <div class="modal-header">
                <button type="reset" class="close" id="generate_tracking_identifiers_dismiss_button_1" name="generate_tracking_identifiers_dismiss_button" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <center><h4 class="modal-title" id="addTransactions">Generate Tracking Identifiers</h4></center>
            </div>
            <div class="modal-body">
                <div class="container-fluid">

                    <&| /util/workflow.mas, id=> "add_transactions_workflow" &>
                        <&| /util/workflow.mas:step, title=> "Intro" &>
                            <& /page/page_title.mas, title=>"Tracking Identifier" &>

                            <p>....... </p>
                            <ul>
                                <li>.......</li>
                                <li>........</li>
                            </ul>
                            <hr>

                            <center>
                                <button id="next_step_add_new_intro" class="btn btn-primary" onclick="Workflow.complete(this);">Go to Next Step</button>
                            </center>
                        </&>

                        <form class="form-horizontal" role="form" name="generate_tracking_identifiers_form" id="generate_tracking_identifiers_form">
                            <&| /util/workflow.mas:step, title=> "Generate Tracking Identifiers" &>
                                <& /page/page_title.mas, title=>"" &>
                                <br/>
                                <br/>
                                <div class="form-horizontal">
                                    <div class="form-group">
                                        <label class="col-sm-4 control-label">Material Type: </label>
                                        <div class="col-sm-8">
                                            <div class="input-group">
                                                <select class="form-control" id="material_type">
                                                    <option value="">Please select a material type </option>
                                                    <option value="accessions">accession</option>
                                                    <option value="seedlots">seedlot</option>
                                                </select>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label col-sm-4">List of Items:</label>
                                        <div class="col-sm-8" >
                                            <div class="input-group">
                                                <select class="form-control" id="item_list_select"></select>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <br/>
                                <div id="form_footer" style="display:none" >
                                    <center>
                                        <button type="button" class="btn btn-primary" onclick="Workflow.complete(this, false);" name="generate_tracking_identifiers_submit" id="generate_tracking_identifiers_submit">Submit</button>
                                    </center>
                                </div>
                            </&>
                        </form>

                        <&| /util/workflow.mas:complete, title=> "Complete" &>
                            <& /page/page_title.mas, title=>"Finished! Tracking Identifiers were stored" &>
                            <p>
                                <span class="ui-icon ui-icon-circle-check" style="float: left; margin: 0 7px 50px 0;"></span>
                                New tracking identifiers were generated successfully
                            </p>
                            <br/>
                        </&>
                    </&><!-- End of workflow -->
                </div>
            </div>
            <div class="modal-footer">
                <button id="generate_tracking_identifiers_dismiss_button_2" name="generate_tracking_identifiers_dismiss_button" type="button" class="btn btn-default" data-dismiss="modal">Close</button>
            </div>
        </div>
    </div>
</div>

<div class="modal fade" id="generate_tracking_identifiers_saved_dialog_message" name="generate_tracking_identifiers_saved_dialog_message" tabindex="-1" role="dialog" aria-labelledby="generateTrackingIdentifiersSavedDialog">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title" id="generateTrackingIdentifiersSavedDialog">Success</h4>
            </div>
            <div class="modal-body">
                <div class="container-fluid">
                    <p>
                        <span class="ui-icon ui-icon-circle-check" style="float: left; margin: 0 7px 50px 0;"></span>
                        The tracking identifier for you materials were generated successfully.
                    </p>
                </div>
            </div>
            <div class="modal-footer">
                <button id="dismiss_generate_tracking_identifiers_saved_dialog" type="button" class="btn btn-default" data-dismiss="modal">Close</button>
            </div>
        </div>
    </div>
</div>


<script>

jQuery(document).ready(function(){

    jQuery("#generate_identifiers_link").click(function(){
        jQuery("#generate_tracking_identifiers_dialog").modal("show");
    });


    jQuery("#material_type").change(function() {
        var selected_material_type = jQuery('#material_type').val();

        var lo = new CXGN.List();
        jQuery('#item_list_select').html(lo.listSelect('item_list_select', [selected_material_type], 'Select a list of materials', undefined, undefined));
    })


});



</script>
